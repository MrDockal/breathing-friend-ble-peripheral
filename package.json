{
  "name": "breathing-friend-ble-peripheral",
  "version": "0.0.1",
  "description": "Project that mocks BLE device",
  "engineStrict": true,
  "engines": {
    "node": ">= 10.13 <11"
  },
  "devDependencies": {
    "@types/bleno": "^0.4.1",
    "@types/jest": "~23.3.2",
    "@types/node": "~10",
    "jest": "~23.6.0",
    "prettier": "1.14.3",
    "rimraf": "~2.6.2",
    "ts-jest": "~23.10.2",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-microsoft-contrib": "~5.2.1",
    "tsutils": "~3.0.0",
    "typescript": "~3.1.0"
  },
  "scripts": {
    "start": "./tools/before-run.sh && npm run clean && npm run build && node build/src/main.js",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jan Dockal <dockal07@gmail.com>",
  "dependencies": {
    "bleno": "^0.5.0",
    "tslib": "~1.9.3"
  }
}
